{
    "name": "xorgxx/neox-firegeolocator-bundle",
    "description": "GeolocatorBundle - Restrict access to routes/controllers based on geolocation (classic structure).",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/config": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/cache": "^7.0",
        "symfony/lock": "^7.0",
        "symfony/security-bundle": "^7.3.0",
        "monolog/monolog": "^3.0",
        "doctrine/dbal": "^3.6",
        "doctrine/doctrine-bundle": "^2.15.1",
        "doctrine/annotations": "^2.0",
        "doctrine/orm": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Neox\\FireGeolocatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neox\\FireGeolocatorBundle\\tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "symfony/phpunit-bridge": "^7.1",
        "friendsofphp/php-cs-fixer": "^3.68",
        "phpstan/phpstan": "^1.11"
    },
    "scripts": {
        "test": "@php -d zend.assertions=1 -d assert.exception=1 vendor/bin/phpunit -c phpunit.xml.dist",
        "test:coverage": "@php -d xdebug.mode=coverage -d zend.assertions=1 -d assert.exception=1 vendor/bin/phpunit -c phpunit.xml.dist --coverage-clover var/coverage.xml --coverage-text",
        "cs-fix": "@php vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php",
        "lint": "@php vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --dry-run --diff",
        "analyse": "@php vendor/bin/phpstan analyse --configuration=phpstan.neon.dist"
    }
}
